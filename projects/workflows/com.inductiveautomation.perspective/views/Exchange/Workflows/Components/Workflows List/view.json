                    "config": {
                      "script": "\tret \u003d exchange.workflows.api.service.start(\n\t\t\tworkflowName\u003dself.view.custom.selectedWorkflowUUID,\n\t\t\ttimeoutSeconds\u003d120\n\t)\n\tif ret.get(\"ok\"):\n\t\texchange.workflows.toast.show(message\u003dret.get(\"message\", \"started!\"), type\u003d\"success\")\n\telse:\n\t\texchange.workflows.toast.show(message\u003dret.get(\"message\", \"failed!\"), type\u003d\"error\")"
                    },
                    "scope": "G",
                    "type": "script"
                  },
