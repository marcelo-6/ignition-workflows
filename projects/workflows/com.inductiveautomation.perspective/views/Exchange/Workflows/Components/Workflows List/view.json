        "config": {
          "path": "/root/Filters/Status.props.value"
        },
        "type": "property"
      }
    },
    "custom.filters.timeRange": {
      "binding": {
        "config": {
          "path": "/root/Filters/TimeRange.props.value"
        },
        "type": "property"
      }
    },
    "custom.filters.version": {
      "binding": {
        "config": {
          "path": "/root/Filters/Version.props.text"
        },
        "type": "property"
      }
    },
    "custom.isWorkflowSelected": {
      "binding": {
        "config": {
          "expression": "len({/root/Filters/Dropdown.props.value}) \u003e 0"
        },
        "type": "expr"
      },
      "persistent": true
    },
    "custom.paging": {
      "persistent": true
    },
    "custom.selectedWorkflowUUID": {
      "persistent": true
    },
    "custom.totalWorkflowsCount": {
      "binding": {
        "config": {
          "expression": "if(isBadOrError({/root/Table.props.data}),\"\", if(isNull(len({/root/Table.props.data})),\"\",len({/root/Table.props.data}) + \" workflows total\"))"
        },
        "type": "expr"
      },
      "persistent": true
    },
    "params.selectedUUID": {
      "binding": {
        "config": {
          "path": "view.custom.selectedWorkflowUUID"
