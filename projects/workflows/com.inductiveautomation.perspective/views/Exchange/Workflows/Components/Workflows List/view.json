    "custom.filters.fromEpochMs": {
      "binding": {
        "config": {
          "path": "view.custom.filters.timeRange"
        },
        "transforms": [
          {
            "code": "\t# value \u003d number of hours in the past\n\tif value is not None and value \u003e 0:\n\t\tpast \u003d system.date.addHours(system.date.now(), -value)\n\t\treturn system.date.toMillis(past)\n\telse:\n\t\t# -1 or anything nonâ€‘positive means \"since epoch\"\n\t\treturn 0",
            "type": "script"
          }
        ],
        "type": "property"
      }
    },
    "custom.filters.id": {
      "binding": {
        "config": {
          "path": "/root/Filters/ID.props.text"
        },
        "type": "property"
      }
    },
    "custom.filters.name": {
      "binding": {
        "config": {
          "path": "/root/Filters/Name.props.text"
        },
        "type": "property"
      }
    },
    "custom.filters.status": {
