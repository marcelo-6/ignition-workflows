{
  "custom": {},
  "params": {
    "noIcon": false
  },
  "propConfig": {
    "params.noIcon": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 40,
      "width": 250
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Icon"
            },
            "position": {
              "basis": "40px",
              "shrink": 0
            },
            "propConfig": {
              "position.display": {
                "binding": {
                  "config": {
                    "expression": "!{view.params.noIcon}"
                  },
                  "type": "expr"
                }
              }
            },
            "props": {
              "path": "material/access_time",
              "style": {
                "padding": "8px"
              }
            },
            "type": "ia.display.icon"
          },
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "basis": "50px",
              "grow": 1
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "expression": "dateFormat(now(30000),\"dd-MMM-YY hh:mm\")"
                  },
                  "type": "expr"
                }
              }
            },
            "props": {
              "style": {
                "overflow": "hidden"
              },
              "textStyle": {
                "paddingRight": 8,
                "whiteSpace": "nowrap"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "FlexContainer",
          "tooltip": {
            "enabled": true,
            "text": "System Time"
          }
        },
        "position": {
          "grow": 1
        },
        "props": {
          "style": {
            "overflow": "hidden"
          }
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "domId": "nav-theme",
      "name": "root"
    },
    "props": {
      "style": {
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}